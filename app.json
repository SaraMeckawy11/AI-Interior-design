{
  "expo": {
    "name": "Roomify",
    "slug": "Roomify",
    "scheme": "roomify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.roomify.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.roomify.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "extra": {
      "eas": {
        "projectId": "7e16d559-0ada-4217-89da-a4aae3692d94"
      }
    },
    "plugins": [
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.365853441307-c1frc7jb4tt7custuqaqn5ua7hnpu7i8"
        }
      ],
      "expo-router",
      "expo-web-browser",
      "expo-secure-store",
      "expo-image-picker",
      "expo-media-library",
      "expo-file-system",
      "expo-localization"
    ]
  }
}
