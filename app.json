{
  "expo": {
    "name": "Livinai",
    "slug": "Livinai",
    "scheme": "livinai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon1.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.jpg"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.livinai.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.livinai.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon1.png",
        "backgroundColor": "#ffffff"
      }
    },
    "extra": {
      "eas": {
        "projectId": "75932d07-1ba3-43be-ae06-e2494b4c805a"
      }
    },
    "plugins": [
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.365853441307-c1frc7jb4tt7custuqaqn5ua7hnpu7i8"
        }
      ],
      "expo-router",
      "expo-web-browser",
      "expo-secure-store",
      "expo-image-picker",
      "expo-media-library",
      "expo-file-system",
      "expo-localization"
    ]
  }
}
